{
  "name": "progress-tracker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.5",
    "@meanie/mongoose-to-json": "^2.4.0",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-server": "^2.9.4",
    "apollo-server-express": "^2.12.0",
    "bootstrap": "^4.3.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.3",
    "node-sass": "^4.12.0",
    "nodemailer": "^6.4.2",
    "react": "^16.11.0",
    "react-apollo": "^3.1.2",
    "react-bootstrap": "^1.0.0-beta.12",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.1",
    "react-scripts": "^3.4.1",
    "zxcvbn": "^4.4.2"
  },
  "homepage": "https://tetsuba.github.io/",
  "scripts": {
    "start": "react-scripts start",
    "start:server:client": "npm run server:test & npm start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --testPathPattern=\"./src/client/\"",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "test:nowatch": "react-scripts test --watchAll=false --env=jsdom --testPathPattern=\"./src/client/\"",
    "test:integration": "env-cmd -f ./.env.testing react-scripts test --watchAll=false --env=node --testPathPattern=\"./src/server/\"",
    "eject": "react-scripts eject",
    "server": "node src/server/server.js",
    "server:dev": "env-cmd -f ./.env.development yarn server",
    "server:test": "env-cmd -f ./.env.testing yarn server",
    "server:prod": "env-cmd yarn server",
    "cy:open": "cypress open",
    "cy:headless": "cypress run",
    "functional": "yarn server:test & yarn start & cypress open",
    "functional:headless": "start-server-and-test start:server:client http://localhost:3000 cy:headless",
    "functional:test": "start-server-and-test server:test http://localhost:3000 cy:headless",
    "pretty": "prettier \"src/server/**/*.js\" \"src/client/**/*.js\" \"src/client/**/*.jsx\" \"cypress/**/*.js\"",
    "pretty:check": "yarn pretty --check",
    "pretty:write": "yarn pretty --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern \"src/server/**/*.js\" \"src/client/**/*.js\" \"src/client/**/*.jsx\" \"cypress/**/*.js\"",
      "pre-push": "yarn test:nowatch"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.3",
    "@testing-library/jest-dom": "^4.2.0",
    "@testing-library/react": "^9.3.2",
    "apollo-server-testing": "^2.12.0",
    "cypress": "^4.1.0",
    "cypress-intellij-reporter": "^0.0.4",
    "env-cmd": "^10.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "husky": "^4.2.5",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "jsdom": "^15.2.1",
    "prettier": "2.0.4",
    "pretty-quick": "^2.0.1",
    "react-test-renderer": "16.13.1",
    "start-server-and-test": "^1.10.11"
  },
  "proxy": "http://localhost:4000"
}
